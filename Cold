import java.util.*;

public class lessthan0 {
    public static void main(String[] args){
        int out = 0;
        Scanner in = new Scanner(System.in);
        for(int i = in.nextInt(); i > 0; i--){
            if(in.nextInt() < 0){
                out++;
            }   
        }
    System.out.println(out);
    }
}
